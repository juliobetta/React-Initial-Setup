{
  "name": "react-initial-setup",
  "version": "0.0.1",
  "productName": "React Initial Setup",
  "main": "app/index.js",
  "author": {
    "name": "Julio Betta",
    "email": "juliobetta@gmail.com",
    "url": "https://github.com/juliobetta"
